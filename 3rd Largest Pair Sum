#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    int b[4]; // FOR STORING TOP 4
    for(int i=0;i<n;i++) cin>>a[i];
    int maxi,ind;
    int count=0;
    b[-1]=INT_MIN;
    b[-2]=INT_MIN;
    b[-3]=INT_MIN;

 for(int j=0;j<4;j++) //LOOP FOR TOP 4
 {
    maxi=INT_MIN;
    ind=-1;
    for(int i=0;i<n;i++)
    {
     if(a[i]>maxi&&a[i]!=b[j-1]&&a[i]!=b[j-2]&&a[i]!=b[j-3])
     {

         maxi=a[i];
         ind=i;
     }
    }
    //FINDING MAXIMUM ELEMENT AND ENSURING IT IS DISTINCT
     if(ind!=-1){
         count++;
    cout<<maxi<<endl;
         b[j]=maxi;
         a[ind]=INT_MIN;
     }
     else break;
    }
 // NOW FINDING THIRD MAXIMUM SUM

set<int> s;

    for(int i=0;i<count;i++)
    {
        for(int j=i+1;j<count;j++)
        {
            s.insert(b[i]+b[j]);
        }
    }
    if(s.size()<3) cout<<"-1\n";
    else
    {
     cout<<(*(++(++s.begin())))<<endl;
    }

}
